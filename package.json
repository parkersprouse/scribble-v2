{
  "name": "scribble-server",
  "version": "1.0.0",
  "engines": {
    "node": "12.16.1",
    "npm": "6.13.4"
  },
  "scripts": {
    "start": "node server",
    "lint": "eslint --ext .js server/",
    "lint:fix": "eslint --ext .js server/ --fix",
    "db:create": "node server/bin/setup_db",
    "dev-build": "cd client && npm run build && cd .. && node server",
    "dev": "concurrently \"npm start\" \"cd client && npm start\" ",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "dependencies": {
    "@sentry/node": "*",
    "bcrypt": "4.0.0",
    "cookie-parser": "*",
    "dotenv": "*",
    "express": "*",
    "express-validator": "*",
    "helmet": "*",
    "jsonwebtoken": "*",
    "luxon": "*",
    "morgan": "*",
    "pg": "*",
    "pg-hstore": "*",
    "sequelize": "*"
  },
  "devDependencies": {
    "concurrently": "*",
    "eslint": "*",
    "eslint-config-airbnb-base": "*",
    "eslint-plugin-import": "*"
  }
}
